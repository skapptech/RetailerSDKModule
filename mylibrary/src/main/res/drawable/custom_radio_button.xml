<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="#ffffff" /> <!-- Change the color as needed -->
            <size android:width="20dp" android:height="20dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <stroke android:color="#D7DBEC" android:width="1dp" /> <!-- Change the color and width as needed -->
            <size android:width="20dp" android:height="20dp" /> <!-- Adjust size as needed -->
        </shape>
    </item>
</layer-list>